{
  "name": "demo_setup",
  "version": "1.0.0",
  "description": "A project for demo setup using typescript",
  "main": "index.js",
  "scripts": {
    "test": "jest && cd client && npm run test",
    "start": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
